<%- include('header', {
  css: [
    `${base}/css/error.css`
  ]
}); %>
<div class="page not found">
  <div class="ui main grid container">
    <div class="two wide column"></div>
    <div class="ten wide column">
      <div class="ui negative message">
        <div class="header">
          # <%= status %>
        </div>
        <p><%= msg %></p>
        <pre><%= err.stack %></pre>
      </div>
    </div>

    <%# --- Bottom Image --- %>
    <div class="sixteen wide column">
      <img src="<%= base %>/images/index-bottom.png" class="ui fluid image">
    </div>
  </div>
</div>
<%- include('footer', {
  js: []
}); %>